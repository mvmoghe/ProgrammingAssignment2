## functions do

## Write a short comment describing this function

makeCacheMatrix <- function(x = matrix()) {
      cacheSolve = function() 
        print(solve(x))
        x <- rbind(c(1, .45), c(.45,1)) ##(A)
        print(x)
        cacheSolve() }
makeCacheMatrix()
 
